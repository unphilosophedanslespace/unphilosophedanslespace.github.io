@article{ruthenberg_acidity_2017,
	title = {Acidity: {Modes} of characterization and quantification},
	volume = {65-66},
	issn = {0039-3681},
	shorttitle = {Acidity},
	doi = {10.1016/j.shpsa.2017.04.003},
	abstract = {This paper provides an account of early historical developments in the characterization and quantification of acidity, which may be considered preliminary steps leading to the measurement of acidity. In this "pre-history" of acidity measurement, emphasis is laid on the relative independence of the rich empirical knowledge about acids from theories of acidity. Many attempts were made to compare and assess the strengths of various acids, based on concrete laboratory operations. However, at least until the arrival of the pH measure, the quantification attempts failed to produce anything qualifying as a measurement scale of a recognizable type. It is doubtful whether even pH qualifies as a true measure of acidity, when the full meaning of acidity is taken into account.},
	language = {eng},
	journal = {Studies in History and Philosophy of Science},
	author = {Ruthenberg, Klaus and Chang, Hasok},
	year = {2017},
	pmid = {29195645},
	keywords = {Acid, Chemical concepts, Measurement, pH, Qualitative knowledge, Quantification},
	pages = {121--131},
	file = {Version soumise:/home/benj/Zotero/storage/XX2SZ74K/Ruthenberg et Chang - 2017 - Acidity Modes of characterization and quantificat.pdf:application/pdf},
}

